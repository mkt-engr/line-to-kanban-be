# サーバー設定
PORT=8080

# LINE認証情報
LINE_CHANNEL_SECRET=your_channel_secret_here
LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token_here

# CockroachDB接続情報
# x-migrations-table-lock=false: CockroachDBはpg_advisory_lock()をサポートしていないため
DATABASE_URL=postgresql://username:password@host:26257/defaultdb?sslmode=verify-full&x-migrations-table-lock=false


